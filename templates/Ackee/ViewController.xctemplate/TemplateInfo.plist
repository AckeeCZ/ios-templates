<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>AllowedTypes</key>
	<array>
		<string>public.swift-source</string>
	</array>
	<key>MainTemplateFiles</key>
	<string>___FILEBASENAME___.swift</string>
	<key>Kind</key>
	<string>Xcode.IDEFoundation.TextSubstitutionFileTemplateKind</string>
	<key>Description</key>
	<string>A class implementing a View Controller with injected View Model </string>
	<key>Summary</key>
	<string>A class implementing a View Controller with injected View Model </string>
	<key>SortOrder</key>
	<integer>2</integer>
	<key>DefaultCompletionName</key>
	<string>ViewController</string>
	<key>Platforms</key>
	<array>
		<string>com.apple.platform.iphoneos</string>
	</array>
	<key>Options</key>
	<array>
		<dict>
			<key>Description</key>
			<string>Base name of the View Controller</string>
			<key>Identifier</key>
			<string>productName</string>
			<key>Name</key>
			<string>View controller base name:</string>
			<key>NotPersisted</key>
			<true/>
			<key>Required</key>
			<true/>
			<key>Type</key>
			<string>text</string>
		</dict>
		<dict>
			<key>Description</key>
			<string>What class to subclass in the new file</string>
			<key>Identifier</key>
			<string>controllerSubclass</string>
			<key>Name</key>
			<string>Subclass of:</string>
			<key>Type</key>
			<string>text</string>
			<key>Default</key>
			<string>BaseViewController</string>
			<key>NotPersisted</key>
			<true/>
		</dict>
		<dict>
			<key>Description</key>
			<string>Is BaseViewController generic?</string>
			<key>Identifier</key>
			<string>isGeneric</string>
			<key>Name</key>
			<string>Is generic</string>
			<key>Type</key>
			<string>checkbox</string>
		</dict>
		<dict>
			<key>Description</key>
			<string>Name of the View Model Protocol</string>
			<key>Identifier</key>
			<string>viewModel</string>
			<key>Name</key>
			<string>View model protocol name:</string>
			<key>Default</key>
			<string>ViewModeling</string>
			<key>Type</key>
			<string>text</string>
			<key>NotPersisted</key>
			<true/>
		</dict>
		
	</array>
</dict>
</plist>
